@{
    ViewBag.Title = "Controle de Abastecimento de Veiculo";
}

<h2>@ViewBag.Title.</h2>
<h3>Descrição do projeto</h3>
<blockquote>
    <strong>Objetivo:</strong>
    <br/>
    <em>Aprimorar os conhecimentos da plataforma ASP.Net MVC com a linguagem C#</em>
</blockquote>
<blockquote>
    <strong>Curso: </strong>
    <br/>
    <em>Informatica para Web</em>
</blockquote>
<blockquote>
    <strong>Professor:  </strong>
    <br />
    <em>Murilo</em>
</blockquote>

<blockquote>
    <strong>Aluno:   </strong>
